{
  "name": "minr-desktop",
  "version": "0.1.2-test.5",
  "description": "A tool to support the workflow of developers on desktop environments.",
  "main": "./out/main/index.js",
  "author": "msato@altus5.com",
  "homepage": "https://github.com/minr-dev/desktop",
  "scripts": {
    "dotenv": "shx cp .env.example .env",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "test:main": "jest --config jest.config.node.js",
    "test:renderer": "jest --config jest.config.web.js",
    "test": "npm run test:main && npm run test:renderer"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.136",
    "@mui/material": "^5.15.14",
    "@mui/styled-engine-sc": "^5.12.0",
    "@mui/x-charts": "^7.3.0",
    "@mui/x-date-pickers": "^6.10.2",
    "@seald-io/nedb": "^4.0.4",
    "@types/winston": "^2.4.4",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "electron-updater": "^6.3.0",
    "googleapis": "^120.0.0",
    "inversify": "^6.0.1",
    "node-window-manager": "^2.2.4",
    "notistack": "^3.0.1",
    "openid-client": "^5.7.0",
    "react-big-calendar": "^1.8.1",
    "react-color": "^2.19.3",
    "react-hook-form": "^7.45.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "react-rnd": "^10.4.1",
    "react-router-dom": "^6.14.0",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.3.11",
    "tinycolor2": "^1.6.0",
    "utf-8-validate": "^6.0.3",
    "uuid": "^9.0.0",
    "winston": "^3.15.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.3.0",
    "@types/jest": "^29.5.3",
    "@types/nedb": "^1.8.13",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "electron": "^30.2.0",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-devtools": "^5.3.1",
    "react-dom": "^18.2.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3",
    "vite": "^4.5.3"
  }
}
